{
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wordBasedSuggestions": false
  },
  "[mdx]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wordBasedSuggestions": false
  },
  "[nunjucks]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wordBasedSuggestions": false
  },
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": true,
  "cSpell.enableFiletypes": ["mdx"],
  "cSpell.words": [
    "bootloader",
    "catgirl",
    "gatekeeping",
    "gatekeepy",
    "XeblogConv"
  ],
  "[markdown][mdx][nunjucks]": {
    "editor.wordBasedSuggestions": "off"
  }
}
